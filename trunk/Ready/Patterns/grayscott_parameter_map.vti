<?xml version="1.0"?>
<VTKFile type="ImageData" version="0.1" byte_order="LittleEndian" compressor="vtkZLibDataCompressor">
  <RD format_version="2">
    <description>
        A map of the parameters of the Gray-Scott system. Here chemical 'c' increases from left to 
        right and controls the 'k' parameter, and chemical 'd' increases from bottom to top and 
        controls the 'F' parameter. For more detail, see:
        &lt;a href=&quot;http://mrob.com/pub/comp/xmorphia/index.html&quot;&gt;http://mrob.com/pub/comp/xmorphia/index.html&lt;/a&gt;
    </description>

    <rule type="formula" name="Gray-Scott">
        
      <param name="timestep"> 0.2    </param>
      <param name="diff">     0.07  </param>

      <formula number_of_chemicals="4">
        delta_a = 2.0f * diff * laplacian_a - a*b*b + d*(1.0f-a);
        delta_b = diff * laplacian_b + a*b*b - (d+c)*b;
      </formula>
      
    </rule>
    
    <initial_pattern_generator apply_when_loading="true">
      <overlay chemical="a">
        <overwrite />
        <constant value="1" />
        <everywhere />
      </overlay>
      <overlay chemical="b">
        <overwrite />
        <white_noise low="0" high="1" />
        <rectangle>
          <Point3D x="0.2" y="0.2" z="0.6" />
          <Point3D x="0.4" y="0.5" z="0.8" />
        </rectangle>
      </overlay>
      <overlay chemical="c">
        <overwrite />
        <linear_gradient val1="0.03" x1="0" y1="0" z1="0" val2="0.07" x2="1" y2="0" z2="0" />
        <everywhere />
      </overlay>
      <overlay chemical="d">
        <overwrite />
        <linear_gradient val1="0" x1="0" y1="0" z1="0" val2="0.1" x2="0" y2="1" z2="0" />
        <everywhere />
      </overlay>
    </initial_pattern_generator>
    
    <render_settings>
        <show_displacement_mapped_surface value="false" />
        <show_multiple_chemicals value="false" />
        <active_chemical value="b" />
        <timesteps_per_render value="4000" />
    </render_settings>
    
  </RD>
  <ImageData WholeExtent="0 511 0 255 0 0" Origin="0 0 0" Spacing="1 1 1">
    <Piece Extent="0 511 0 255 0 0">
      <PointData>
        <DataArray type="Float32" Name="a" format="appended" RangeMin="0" RangeMax="0" offset="0" />
        <DataArray type="Float32" Name="b" format="appended" RangeMin="0" RangeMax="0" offset="1216" />
        <DataArray type="Float32" Name="c" format="appended" RangeMin="0" RangeMax="0" offset="2432" />
        <DataArray type="Float32" Name="d" format="appended" RangeMin="0" RangeMax="0" offset="3648" />
      </PointData>
      <CellData>
      </CellData>
    </Piece>
  </ImageData>
  <AppendedData encoding="base64">
   _
  </AppendedData>
</VTKFile>
