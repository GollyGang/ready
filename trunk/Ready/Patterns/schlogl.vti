<?xml version="1.0"?>
<VTKFile type="ImageData" version="0.1" byte_order="LittleEndian" compressor="vtkZLibDataCompressor">
    
  <RD format_version="1">
      
    <rule type="formula" name="Schlogl" number_of_chemicals="1">
      <description>
        Schlögl’s model is the canonical example of a chemical reaction system that exhibits bistability.
        Schlögl, F. (1972) Chemical reaction models for non-equilibrium phase transition. Z. Physik. 253, 147–161.
      </description>
      <param name="timestep">0.1</param>
      <formula>
        delta_a = laplacian_a + a - a*a*a;
      </formula>
    </rule>
    
    <pattern_description>
        The initial pattern is simply random values at each pixel, between -0.005 and 0.005. This low-amplitude noise quickly develops into large areas of matching values.
    </pattern_description>
    
    <initial_pattern_generator apply_when_loading="1">
        <overlay chemical="a">
            <overwrite />
            <white_noise low="-0.005" high="0.005" />
            <everywhere />
        </overlay>
    </initial_pattern_generator>
    
  </RD>
  
  <ImageData WholeExtent="0 127 0 63 0 0" Origin="0 0 0" Spacing="1 1 1">
    <Piece Extent="0 127 0 63 0 0">
      <PointData Scalars="Scalars_">
        <DataArray type="Float32" Name="Scalars_" format="appended" RangeMin="0" RangeMax="0" offset="0" />
      </PointData>
      <CellData>
      </CellData>
    </Piece>
  </ImageData>
  <AppendedData encoding="base64">
   _AQAAAACAAAAAAAAANAAAAA==eJztwQEBAAAAgJD+r+4ICgAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAYgAAAAQ==
  </AppendedData>
</VTKFile>
