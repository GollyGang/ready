<?xml version="1.0"?>
<VTKFile type="ImageData" version="0.1" byte_order="LittleEndian" compressor="vtkZLibDataCompressor">
  <RD format_version="1">
      
    <description>
        Meinhardt stripes
    </description>
    
    <rule type="formula" name="Meinhardt">
        
      <param name="timestep">0.1</param>
      <param name="k_ab">0.04</param>
      <param name="k_c">0.06</param>
      <param name="k_de">0.04</param>
      <param name="diff1">0.009</param>
      <param name="diff2">0.2</param>
      <param name="arand">0.01</param>
      
      <formula number_of_chemicals="6">
        delta_a = 0.01f * a * a * e * f / c - a * k_ab + diff1 * laplacian_a;
        delta_b = 0.01f * b * b * d / c - b * k_ab + diff1 * laplacian_b;
        delta_c = 0.01f * a * a * e * f + 0.01f * b * b * d - c * k_c;
        delta_d = (a - d) * k_de + diff2 * laplacian_d;
        delta_e = (b - e) * k_de + diff2 * laplacian_e;
        delta_f = 0.0f;
      </formula>
      
    </rule>
    
    <initial_pattern_generator apply_when_loading="true">
        <!-- a = k_c / (2*k_ab) -->
        <overlay chemical="a">
            <overwrite />
            <parameter name="k_c" />
            <everywhere />
        </overlay>
        <overlay chemical="a">
            <divide />
            <constant value="2" />
            <everywhere />
        </overlay>
        <overlay chemical="a">
            <divide />
            <parameter name="k_ab" />
            <everywhere />
        </overlay>
        <!-- b = a -->
        <overlay chemical="b">
            <overwrite />
            <other_chemical chemical="a" />
            <everywhere />
        </overlay>
        <!-- c = 0.02*a*a*a/k_c -->
        <overlay chemical="c">
            <overwrite />
            <other_chemical chemical="a" />
            <everywhere />
        </overlay>
        <overlay chemical="c">
            <multiply />
            <other_chemical chemical="a" />
            <everywhere />
        </overlay>
        <overlay chemical="c">
            <multiply />
            <other_chemical chemical="a" />
            <everywhere />
        </overlay>
        <overlay chemical="c">
            <divide />
            <parameter name="k_c" />
            <everywhere />
        </overlay>
        <overlay chemical="c">
            <multiply />
            <constant value="0.02" />
            <everywhere />
        </overlay>
        <!-- d = a -->
        <overlay chemical="d">
            <overwrite />
            <other_chemical chemical="a" />
            <everywhere />
        </overlay>
        <!-- e = a -->
        <overlay chemical="e">
            <overwrite />
            <other_chemical chemical="a" />
            <everywhere />
        </overlay>
        <!-- f = 1+[-0.5*arand,0.5*arand] in some places, 1 elsewhere -->
        <overlay chemical="f">
            <overwrite />
            <constant value="1" />
            <everywhere />
        </overlay>
        <overlay chemical="f">
            <overwrite />
            <white_noise low="-0.5" high="0.5" />
            <rectangle>
                <point3D x="0" y="0" z="0" />
                <point3D x="0.25" y="1" z="1" />
            </rectangle>
        </overlay>
        <overlay chemical="f">
            <multiply />
            <parameter name="arand" />
            <rectangle>
                <point3D x="0" y="0" z="0" />
                <point3D x="0.25" y="1" z="1" />
            </rectangle>
        </overlay>
        <overlay chemical="f">
            <add />
            <constant value="1" />
            <rectangle>
                <point3D x="0" y="0" z="0" />
                <point3D x="0.25" y="1" z="1" />
            </rectangle>
        </overlay>
    </initial_pattern_generator>
    
    <render_settings>
        <low value="0" />
        <high value="1.5" />
        <show_multiple_chemicals value="false" />
    </render_settings>
    
  </RD>
  <ImageData WholeExtent="0 127 0 63 0 0" Origin="0 0 0" Spacing="1 1 1">
    <Piece Extent="0 127 0 63 0 0">
      <PointData Scalars="Scalars_">
        <DataArray type="Float32" Name="Scalars_" NumberOfComponents="6" format="appended" RangeMin="0" RangeMax="0" offset="0" />
      </PointData>
      <CellData>
      </CellData>
    </Piece>
  </ImageData>
  <AppendedData encoding="base64">
_
  </AppendedData>
</VTKFile>
