        <rectangle>
          <point3d x="0.2" y="0" z="0" />
          <point3d x="0.5" y="0.2" z="0.2" />
        </rectangle>
      </overlay>
     
    </initial_pattern_generator>
    
    <render_settings>
      <low value="-1.5" />
