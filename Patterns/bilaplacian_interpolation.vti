        <rectangle>
          <point3D x="0.55" y="0.2" z="0.4">

          </point3D>
          <point3D x="0.8" y="0.3" z="0.6">

          </point3D>

        </rectangle>

      </overlay>
      <overlay chemical="b">
        <overwrite>

        </overwrite>
        <constant value="1">

        </constant>
        <circle radius="0.1">
          <point3D x="0.7" y="0.8" z="0.6">

