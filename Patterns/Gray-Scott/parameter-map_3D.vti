<?xml version="1.0"?>
<VTKFile type="ImageData" version="0.1" byte_order="LittleEndian" header_type="UInt32" compressor="vtkZLibDataCompressor">
  <RD format_version="5">
    <description>
      A map of the parameters of the Gray-Scott system.

      Here &apos;k&apos; increases (in the default camera view) from left to
      right, &apos;F&apos; increases from bottom to top and &apos;diff&apos; increases from back to front.

      For more detail, see:
      &lt;a href=&quot;http://mrob.com/pub/comp/xmorphia/index.html&quot;&gt;http://mrob.com/pub/comp/xmorphia/index.html&lt;/a&gt;

      The parameters diff1, diff2, k1, k2, F1 and F2 define the current range of the map. Change them to explore.

    </description>
    <rule name="Gray-Scott" type="formula" wrap="0" neighborhood_type="vertex">
      <param name="timestep">
        0.200000
      </param>
      <param name="diff1">
        0.200000
      </param>
      <param name="diff2">
        0.060000
      </param>
      <param name="k1">
        0.055000
      </param>
      <param name="k2">
        0.075000
      </param>
      <param name="F1">
        0.001000
      </param>
      <param name="F2">
        0.087000
      </param>
      <formula number_of_chemicals="2">
        float4 xpos = index_x / (float)X; // [0,1]
        float4 ypos = index_y / (float)Y; // [0,1]
        float4 zpos = index_z / (float)Z; // [0,1]
        float4 k = k1 + (k2-k1)*xpos;
        float4 F = F1 + (F2-F1)*ypos;
        float4 diff = diff1 + (diff2-diff1)*zpos;
        delta_a = 2.0f * diff * laplacian_a - a*b*b + F*(1.0f-a);
        delta_b = diff * laplacian_b + a*b*b - (F+k)*b;
      </formula>
    </rule>
    <initial_pattern_generator apply_when_loading="true" zero_first="false">
      <overlay chemical="a">
        <overwrite/>
        <constant value="1"/>
        <everywhere/>
      </overlay>
      <overlay chemical="b">
        <overwrite/>
        <constant value="0"/>
        <everywhere/>
      </overlay>
      <overlay chemical="b">
        <overwrite/>
        <white_noise low="0" high="1"/>
        <rectangle>
          <point3D x="0.2" y="0.2" z="0.6"/>
          <point3D x="0.4" y="0.5" z="0.8"/>
        </rectangle>
      </overlay>
      <overlay chemical="a">
        <subtract/>
        <other_chemical chemical="b"/>
        <everywhere/>
      </overlay>
    </initial_pattern_generator>
    <render_settings>
      <surface_color r="1" g="1" b="1"/>
      <color_low r="0" g="0" b="1"/>
      <color_high r="1" g="0" b="0"/>
      <show_color_scale value="true"/>
      <show_multiple_chemicals value="false"/>
      <active_chemical value="b"/>
      <low value="0"/>
      <high value="1"/>
      <vertical_scale_1D value="30"/>
      <vertical_scale_2D value="15"/>
      <contour_level value="0.25"/>
      <use_wireframe value="false"/>
      <show_cell_edges value="false"/>
      <show_bounding_box value="true"/>
      <show_chemical_label value="true"/>
      <slice_3D value="true"/>
      <slice_3D_axis value="z"/>
      <slice_3D_position value="0.01"/>
      <show_displacement_mapped_surface value="false"/>
      <color_displacement_mapped_surface value="true"/>
      <use_image_interpolation value="true"/>
      <timesteps_per_render value="25"/>
      <show_phase_plot value="false"/>
      <phase_plot_x_axis value="a"/>
      <phase_plot_y_axis value="b"/>
      <phase_plot_z_axis value="c"/>
    </render_settings>
  </RD>
  <ImageData WholeExtent="0 63 0 63 0 63" Origin="0 0 0" Spacing="1 1 1">
  <Piece Extent="0 63 0 63 0 63">
    <PointData>
      <DataArray type="Float32" Name="a" format="binary" RangeMin="0" RangeMax="0">
        
      </DataArray>
      <DataArray type="Float32" Name="b" format="binary" RangeMin="0" RangeMax="0">
        
      </DataArray>
    </PointData>
    <CellData>
    </CellData>
  </Piece>
  </ImageData>
</VTKFile>
