<?xml version="1.0"?>
<VTKFile type="ImageData" version="0.1" byte_order="LittleEndian" header_type="UInt32" compressor="vtkZLibDataCompressor">
  <RD format_version="5">
    <description>
      Kobayashi, R. (1993) &quot;Modeling and numerical simulations of dendritic crystal growth&quot; Physica D: Nonlinear Phenomena, 63(3-4), 410-423.

      This version is without the anisotropy term and just models the &apos;Laplacian growth&apos;.

      &apos;a&apos; is the &apos;phase field&apos;, which changes from 0=liquid to 1=solid. &apos;b&apos; is the temperature.

      Change parameter &apos;k&apos; between 0.8 and 5.0 to change the shape of the dendrites.
    </description>
    <rule name="Laplacian Growth" type="formula" wrap="0" neighborhood_type="vertex">
      <param name="k">
        2.5
      </param>
      <formula number_of_chemicals="2">
        const float timestep = 0.0001;
        const float dx = 0.03;
        const float dy = 0.03;
        const float tau = 0.0003;
        const float epsilonbar = 0.01;
        const float alpha = 0.9;
        const float gamma = 10.0;
        const float teq = 1.0;
        const float D_a = epsilonbar*epsilonbar/(dx*dx*tau);
        const float D_b = 1.0/(dx*dx);
            
        #define M_PI 3.14159265358979323846   // needed for MacOS

        float4 m = (float4)(alpha / M_PI) * atan(gamma *(teq-b));
        delta_a = D_a*laplacian_a + a*(1.0f-a)*(a-0.5f+m)/tau;
        delta_b = D_b*laplacian_b + k*delta_a;
      </formula>
    </rule>
    <initial_pattern_generator apply_when_loading="true" zero_first="false">
      <overlay chemical="a">
        <overwrite/>
        <white_noise low="0" high="0.01"/>
        <everywhere/>
      </overlay>
      <overlay chemical="a">
        <add/>
        <constant value="0.99"/>
        <rectangle>
          <point3D x="0" y="0" z="0"/>
          <point3D x="1" y="0.01" z="1"/>
        </rectangle>
      </overlay>
      <overlay chemical="a">
        <add/>
        <constant value="1"/>
        <circle radius="0.03">
          <point3D x="0.2" y="0.02" z="0.3"/>
        </circle>
      </overlay>
      <overlay chemical="a">
        <add/>
        <constant value="1"/>
        <circle radius="0.03">
          <point3D x="0.6" y="0.02" z="0.4"/>
        </circle>
      </overlay>
      <overlay chemical="a">
        <add/>
        <constant value="1"/>
        <circle radius="0.03">
          <point3D x="0.3" y="0.02" z="0.8"/>
        </circle>
      </overlay>
      <overlay chemical="b">
        <overwrite/>
        <white_noise low="-1" high="1"/>
        <everywhere/>
      </overlay>
    </initial_pattern_generator>
    <render_settings>
      <surface_color r="1" g="1" b="1"/>
      <color_low r="0" g="0" b="1"/>
      <color_high r="1" g="0" b="0"/>
      <show_color_scale value="false"/>
      <show_multiple_chemicals value="false"/>
      <active_chemical value="a"/>
      <low value="0"/>
      <high value="1"/>
      <vertical_scale_1D value="30"/>
      <vertical_scale_2D value="15"/>
      <contour_level value="0.5"/>
      <cap_contour value="true"/>
      <invert_contour_cap value="false"/>
      <use_wireframe value="false"/>
      <show_cell_edges value="false"/>
      <show_bounding_box value="true"/>
      <show_chemical_label value="true"/>
      <slice_3D value="false"/>
      <slice_3D_axis value="z"/>
      <slice_3D_position value="0.5"/>
      <show_displacement_mapped_surface value="false"/>
      <color_displacement_mapped_surface value="false"/>
      <use_image_interpolation value="true"/>
      <timesteps_per_render value="32"/>
      <show_phase_plot value="false"/>
      <phase_plot_x_axis value="a"/>
      <phase_plot_y_axis value="b"/>
      <phase_plot_z_axis value="c"/>
    </render_settings>
  </RD>
  <ImageData WholeExtent="0 63 0 63 0 63" Origin="0 0 0" Spacing="1 1 1">
  <Piece Extent="0 63 0 63 0 63">
    <PointData>
      <DataArray type="Float32" Name="a" format="binary" RangeMin="0" RangeMax="0">
        
      </DataArray>
      <DataArray type="Float32" Name="b" format="binary" RangeMin="0" RangeMax="0">
        
      </DataArray>
    </PointData>
    <CellData>
