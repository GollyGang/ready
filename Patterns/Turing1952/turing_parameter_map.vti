<?xml version="1.0"?>
<VTKFile type="ImageData" version="0.1" byte_order="LittleEndian" header_type="UInt32" compressor="vtkZLibDataCompressor">
  <RD format_version="5">
    <description>
      &lt;a href=&quot;http://en.wikipedia.org/wiki/Alan_Turing&quot;&gt;Alan Turing&lt;/a&gt; published a paper in 1952 called 
      &lt;a href=&quot;http://en.wikipedia.org/wiki/The_chemical_basis_of_morphogenesis&quot;&gt;The Chemical Basis of Morphogenesis&lt;/a&gt;.
      He showed how chemicals (morphogens) could diffuse through living tissue and drive development in embryos and plants.
      The key result is that &lt;i&gt;instabilities&lt;/i&gt; can arise and form static patterns.

      A parameter map for the system described in Turing's paper. Parameter k varies from left to right. Parameter D_b varies from
      bottom to top. The system can produce high-on-low spots and low-on-high spots, separated by a region of labyrinthine stripes.
    </description>
    <rule name="Turing" type="formula" wrap="0" neighborhood_type="vertex" neighborhood_range="1" neighborhood_weight="laplacian">
      <param name="timestep">
        0.1
      </param>
      <param name="D_a">
        2.5
      </param>
      <param name="k1">
        0.0
      </param>
      <param name="k2">
        0.08
      </param>
      <param name="D_b1">
        0.01
      </param>
      <param name="D_b2">
        0.90
      </param>
      <formula number_of_chemicals="2">
        float4 hpos = index_x / (float)X; // [0,1]
        float4 vpos = index_y / (float)Y; // [0,1]
        float4 k = k1 + (k2-k1)*hpos;
        float4 D_b = D_b1 + (D_b2-D_b1)*vpos;
        delta_a = k * (16.0f - a * b) + D_a * laplacian_a;
        delta_b = max( -b, k * (a * b - b - 12.0f) + D_b * laplacian_b );
      </formula>
    </rule>
    <initial_pattern_generator apply_when_loading="true" zero_first="false">
      <overlay chemical="a">
        <overwrite/>
        <white_noise low="3" high="5"/>
        <everywhere/>
      </overlay>
      <overlay chemical="b">
        <overwrite/>
        <white_noise low="3" high="5"/>
        <everywhere/>
      </overlay>
    </initial_pattern_generator>
    <render_settings>
      <surface_color r="1" g="1" b="1"/>
      <color_low r="0" g="0" b="1"/>
      <color_high r="1" g="0" b="0"/>
      <show_color_scale value="true"/>
      <show_multiple_chemicals value="false"/>
      <active_chemical value="a"/>
      <low value="2"/>
      <high value="7"/>
      <vertical_scale_1D value="30"/>
      <vertical_scale_2D value="15"/>
      <contour_level value="5"/>
      <cap_contour value="true"/>
      <invert_contour_cap value="false"/>
      <use_wireframe value="false"/>
      <show_cell_edges value="false"/>
      <show_bounding_box value="true"/>
      <show_chemical_label value="true"/>
      <slice_3D value="true"/>
      <slice_3D_axis value="z"/>
      <slice_3D_position value="0.5"/>
      <show_displacement_mapped_surface value="false"/>
      <color_displacement_mapped_surface value="true"/>
      <use_image_interpolation value="true"/>
      <timesteps_per_render value="100"/>
      <show_phase_plot value="false"/>
      <phase_plot_x_axis value="a"/>
      <phase_plot_y_axis value="b"/>
      <phase_plot_z_axis value="c"/>
    </render_settings>
  </RD>
  <ImageData WholeExtent="0 511 0 511 0 0" Origin="0 0 0" Spacing="1 1 1">
  <Piece Extent="0 511 0 511 0 0">
    <PointData>
      <DataArray type="Float32" Name="a" format="binary" RangeMin="0" RangeMax="0">
        
      </DataArray>
      <DataArray type="Float32" Name="b" format="binary" RangeMin="0" RangeMax="0">
        
      </DataArray>
    </PointData>
    <CellData>
    </CellData>
  </Piece>
  </ImageData>
</VTKFile>
