<?xml version="1.0"?>
<VTKFile type="ImageData" version="0.1" byte_order="LittleEndian" header_type="UInt32" compressor="vtkZLibDataCompressor">
  <RD format_version="5">
    <description>
      Fig. 4 from Andrew Morozov, Shigui Ruan, Bai-Lian Li (2008) "&lt;a href=&quot;http://www2.le.ac.uk/departments/mathematics/extranet/staff-material/staff-profiles/am379/list-of-papers/multispecies-patchy.pdf&quot;&gt;Patterns of patchy spread in multi-species reactionâ€“diffusion models&lt;/a&gt;" Ecological Complexity, Volume 5, Issue 4, Pages 313-328.
    </description>
    <rule name="Morozov2008" type="formula" wrap="1" neighborhood_type="vertex" neighborhood_range="1" neighborhood_weight="laplacian">
      <param name="timestep">
        0.01
      </param>
      <param name="r">
        2.0
      </param>
      <param name="chi12">
        2.0
      </param>
      <param name="chi21">
        10.0
      </param>
      <param name="gamma">
        2.0
      </param>
      <param name="g">
        1.1
      </param>
      <param name="epsilon1">
        1.0
      </param>
      <param name="epsilon2">
        2.0
      </param>
      <param name="delta">
        0.094
      </param>
      <formula number_of_chemicals="3">
        delta_a = laplacian_a + a*(1.0f-a-chi12*b) - a*c/(1.0f+gamma*a);
        delta_b = epsilon1*laplacian_b + r*b*(1.0f-b-chi21*a);
        delta_c = epsilon2*laplacian_c + g*a*c/(1.0f + gamma*a) - delta*c;
      </formula>
    </rule>
    <initial_pattern_generator apply_when_loading="true" zero_first="false">
      <overlay chemical="a">
        <overwrite/>
        <constant value="0"/>
        <everywhere/>
      </overlay>
      <overlay chemical="a">
        <overwrite/>
        <constant value="1"/>
        <rectangle>
          <point3D x="0.46" y="0.46" z="0.46"/>
          <point3D x="0.54" y="0.54" z="0.54"/>
        </rectangle>
      </overlay>
      <overlay chemical="b">
        <overwrite/>
        <constant value="1"/>
        <everywhere/>
      </overlay>
      <overlay chemical="c">
        <overwrite/>
        <constant value="0"/>
        <everywhere/>
      </overlay>
      <overlay chemical="c">
        <overwrite/>
        <constant value="0.5"/>
        <rectangle>
          <point3D x="0.45" y="0.45" z="0.45"/>
          <point3D x="0.51" y="0.51" z="0.51"/>
        </rectangle>
      </overlay>
    </initial_pattern_generator>
    <render_settings>
      <surface_color r="1" g="1" b="1"/>
      <color_low r="0" g="0" b="1"/>
      <color_high r="1" g="0" b="0"/>
      <show_color_scale value="true"/>
      <show_multiple_chemicals value="true"/>
      <active_chemical value="a"/>
      <low value="0"/>
      <high value="2"/>
      <vertical_scale_1D value="30"/>
      <vertical_scale_2D value="15"/>
      <contour_level value="0.25"/>
      <cap_contour value="true"/>
      <invert_contour_cap value="false"/>
      <use_wireframe value="false"/>
      <show_cell_edges value="false"/>
      <show_bounding_box value="true"/>
      <show_chemical_label value="true"/>
      <slice_3D value="true"/>
      <slice_3D_axis value="z"/>
      <slice_3D_position value="0.5"/>
      <show_displacement_mapped_surface value="false"/>
      <color_displacement_mapped_surface value="false"/>
      <use_image_interpolation value="true"/>
      <timesteps_per_render value="128"/>
      <show_phase_plot value="false"/>
      <phase_plot_x_axis value="a"/>
      <phase_plot_y_axis value="b"/>
      <phase_plot_z_axis value="c"/>
    </render_settings>
  </RD>
  <ImageData WholeExtent="0 255 0 255 0 0" Origin="0 0 0" Spacing="1 1 1">
  <Piece Extent="0 255 0 255 0 0">
    <PointData>
      <DataArray type="Float32" Name="a" format="binary" RangeMin="0" RangeMax="0">
        
      </DataArray>
      <DataArray type="Float32" Name="b" format="binary" RangeMin="0" RangeMax="0">
        
      </DataArray>
      <DataArray type="Float32" Name="c" format="binary" RangeMin="0" RangeMax="0">
        
      </DataArray>
    </PointData>
    <CellData>
    </CellData>
  </Piece>
  </ImageData>
</VTKFile>
